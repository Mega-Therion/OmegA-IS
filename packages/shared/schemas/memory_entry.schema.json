{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "MemoryEntry",
  "type": "object",
  "required": ["id", "namespace", "content", "ts"],
  "properties": {
    "id": { "type": "string" },
    "namespace": { "type": "string" },
    "content": { "type": "string" },
    "ts": { "type": "string", "format": "date-time" },
    "score": { "type": "number" },
    "meta": { "type": "object", "additionalProperties": true }
  },
  "additionalProperties": false
}
